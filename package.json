{
  "name": "rupununi-mining",
  "version": "1.0.0",
  "description": "Gregor MacLennan",
  "main": "index.js",
  "scripts": {
    "build": "browserify web.js > bundle.js",
    "package-win": "electron-packager . 'Rupununi Mining' --out=dist/ --platform=win32 --arch=ia32 --version=0.36.0 --ignore=^dist --prune --overwrite --asar",
    "package-mac": "electron-packager . 'Rupununi Mining' --out=dist/ --platform=darwin --arch=x64 --version=0.36.0 --ignore=^dist --prune --overwrite",
    "preversion": "npm test && npm run build",
    "postinstall": "browserify node_modules/leaflet-side-by-side/index.js > lib/leaflet-side-by-side.js",
    "lint": "standard index.js",
    "start": "budo web.js:bundle.js --live",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/rupununi-mining.git"
  },
  "author": "Gregor MacLennan / Digital Democracy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digidem/rupununi-mining/issues"
  },
  "homepage": "https://github.com/digidem/rupununi-mining#readme",
  "dependencies": {
    "drag-drop": "^2.3.1",
    "filereader-stream": "^1.0.0",
    "fs-blob-store": "gmaclennan/fs-blob-store#patch-1",
    "gunzip-maybe": "^1.2.1",
    "image-cache": "digidem/html5-image-cache#idb-blob-store",
    "leaflet": "^1.0.0-beta.2",
    "leaflet-bing-layer": "^3.0.1",
    "leaflet-fullscreen": "0.0.4",
    "leaflet-hash": "^0.2.1",
    "leaflet-minimap": "github:gmaclennan/leaflet-minimap",
    "lodash.debounce": "^3.1.1",
    "lodash.find": "^3.2.1",
    "lodash.pluck": "^3.1.2",
    "lodash.values": "^3.0.0",
    "mime-types": "^2.1.8",
    "moment": "^2.10.6",
    "object-assign": "^4.0.1",
    "relative-date": "^1.1.2",
    "tar-stream": "^1.3.1",
    "title-case": "^1.1.1"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "budo": "^7.0.1",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.36.0",
    "leaflet-side-by-side": "^2.0.0",
    "standard": "^5.4.1"
  }
}
